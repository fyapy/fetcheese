{
  "name": "fetcheese",
  "version": "1.1.0",
  "description": "HTTP client for browser and node JS environment",
  "keywords": [
    "fetch",
    "http",
    "request",
    "ajax",
    "node"
  ],
  "author": "Amir Abdullin <amir.abdullin.a@gmail.com>",
  "license": "MIT",
  "main": "cjs/index",
  "module": "esm/index",
  "typings": "esm/index",
  "sideEffects": false,
  "files": [
    "esm/*",
    "cjs/*"
  ],
  "scripts": {
    "build": "yarn clean && rollup --config",
    "prepare": "yarn clean && rollup --config",
    "clean": "rimraf ./esm ./cjs",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/express": "^4.17.9",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "26.0.20",
    "@types/node": "^14.14.20",
    "express": "^4.17.1",
    "jest": "26.6.0",
    "jest-environment-jsdom": "26.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "26.4.4",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "isomorphic-fetch": "^3.0.0",
    "node-abort-controller": "^1.1.0",
    "xhr2": "^0.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fyapy/fetcheese.git"
  },
  "bugs": {
    "url": "https://github.com/fyapy/fetcheese/issues"
  },
  "homepage": "https://github.com/fyapy/fetcheese#readme"
}
